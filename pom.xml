<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.seda.cart.webservices</groupId>
	<artifactId>com.seda.cart.webservices</artifactId>
	<version>1.0.0</version>
	<packaging>war</packaging>
	<name>com.seda.cart.webservices</name>
	<description>com.seda.cart.webservices</description>

   <parent>
      <groupId>it.maggioli.informatica.base.pom</groupId>
      <artifactId>pagonet-base</artifactId>
      <version>1.1</version>
      <relativePath/>
   </parent>
	
	<distributionManagement>
		<repository>
		  <uniqueVersion>false</uniqueVersion>
		  <id>google</id>
		  <name>Corporate Repository</name>
		  <url>artifactregistry://europe-west1-maven.pkg.dev/maggioli-registry/pagamenti-releases</url>
		  <layout>default</layout>
		</repository>
	</distributionManagement>
    
	<build>

		<extensions>
			<extension>
			  <groupId>com.google.cloud.artifactregistry</groupId>
			  <artifactId>artifactregistry-maven-wagon</artifactId>
			  <version>2.2.0</version>
			</extension>
		</extensions>


		<sourceDirectory>src</sourceDirectory>
		<resources>
			<resource>
				<directory>src</directory>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.0</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>3.2.1</version>
				<configuration>
					<warSourceDirectory>WebContent</warSourceDirectory>
					<archive>
						<manifestEntries>
							<Logging-Profile>com.seda.cart.webservices</Logging-Profile>
							<Dependencies>com.seda.compatibility,com.seda.commons,org.apache.log4j,com.seda.data:1.0</Dependencies>
<!-- 							<Dependencies>javax.servlet.api,org.apache.axis,com.seda.payer.core,,,com.seda.j2ee5,com.seda.data,com.seda.cart.webservices.client,javax.xml.rpc.api,com.seda.payer.commons</Dependencies> -->
						</manifestEntries>
					</archive>
				</configuration>

			</plugin>
		</plugins>
	</build>

	<dependencies>

		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>javax.servlet-api</artifactId>
			<version>3.0.1</version>
			<scope>compile</scope>
		</dependency>

		<!-- Librerie per Axis 1.4 -->
		<dependency>
			<groupId>org.apache.axis</groupId>
			<artifactId>axis</artifactId>
			<version>1.4</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
		    <groupId>commons-discovery</groupId>
		    <artifactId>commons-discovery</artifactId>
		    <version>0.5</version>
		    <scope>compile</scope>
		</dependency>
		<dependency>
		    <groupId>wsdl4j</groupId>
		    <artifactId>wsdl4j</artifactId>
		    <version>1.6.2</version>
		    <scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>javax.xml.rpc</groupId>
			<artifactId>javax.xml.rpc-api</artifactId>
			<version>1.1</version>
			<scope>compile</scope>
		</dependency>
		<!-- Librerie per Axis 1.4 -->
		
		<dependency>
			<groupId>com.seda.commons</groupId>
			<artifactId>com.seda.commons</artifactId>
			<version>1.0</version>
			<!--  <scope>provided</scope>-->
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>com.seda.compatibility</groupId>
			<artifactId>com.seda.compatibility</artifactId>
			<version>1.0</version>
			<!--  <scope>provided</scope>-->
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>com.seda.payer.core</groupId>
			<artifactId>com.seda.payer.core</artifactId>
			<version>1.0</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.17</version>
			<!--  <scope>provided</scope>-->
			<scope>compile</scope>
		</dependency>
		
		<dependency>
			<groupId>com.seda.j2ee5</groupId>
			<artifactId>com.seda.j2ee5</artifactId>
			<version>1.0</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>com.seda.data</groupId>
			<artifactId>com.seda.data</artifactId>
			<version>1.0</version>
			<!--  <scope>provided</scope>-->
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>com.seda.cart.webservices.client</groupId>
			<artifactId>com.seda.cart.webservices.client</artifactId>
			<version>1.0</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>com.seda.payer.commons</groupId>
			<artifactId>com.seda.payer.commons</artifactId>
			<version>1.0</version>
			<scope>compile</scope>
		</dependency>
		
		<dependency>
			<groupId>javax.xml.soap</groupId>
			<artifactId>javax.xml.soap-api</artifactId>
			<version>1.4.0</version>
			<scope>compile</scope>
		</dependency>

	</dependencies>
	
	
	
	
	
</project>
